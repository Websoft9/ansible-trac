#!/usr/bin/expect -f
password=$(cat /root/password.txt | awk -F ":" '{print $2}')
set timeout 30
set projectname trac
set database mysql://root:$password@localhost:3306/trac
spawn trac-admin /data/wwwroot/trac initenv 
expect {
"Project]>" { send "$projectname\r";exp_continue }
"db]>" { send "$database\r" }
}
interact